{
  "id": "sponsor",
  "label": "Sponsor",
  "repeatable": true,
  "status": true,
  "format": "page",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "Slug", "placeholder": "" }
      },
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Name",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading2"
        }
      },
      "url": {
        "type": "Link",
        "config": {
          "label": "Url",
          "placeholder": "",
          "allowTargetBlank": true,
          "select": null
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,heading3,heading4,heading5,heading6,strong,em,list-item,o-list-item,embed,hyperlink"
        }
      },
      "logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {},
          "thumbnails": [
            { "name": "highlight", "width": 470, "height": 280 },
            { "name": "small", "width": 167, "height": 110 }
          ]
        }
      },
      "date_start": {
        "type": "Date",
        "config": { "label": "Start", "placeholder": "" }
      },
      "date_end": {
        "type": "Date",
        "config": { "label": "End", "placeholder": "" }
      },
      "sponsorship_type": {
        "type": "Select",
        "config": {
          "label": "Type",
          "placeholder": "",
          "options": ["Platinum", "Gold", "Silver", "Bronze"]
        }
      },
      "highlight_video": {
        "type": "Embed",
        "config": { "label": "Highlight", "placeholder": "" }
      }
    }
  }
}
