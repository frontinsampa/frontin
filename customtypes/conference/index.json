{
  "id": "conference",
  "label": "Conference",
  "repeatable": true,
  "status": true,
  "format": "page",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "Slug", "placeholder": "" }
      },
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Name",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading2"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {},
          "thumbnails": [{ "name": "main", "width": 264, "height": 195 }]
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "date_defined": {
        "type": "Boolean",
        "config": {
          "label": "Date defined",
          "default_value": false,
          "placeholder_true": "true",
          "placeholder_false": "false"
        }
      },
      "date": {
        "type": "Date",
        "config": { "label": "Date", "placeholder": "" }
      },
      "subscribe": {
        "type": "Link",
        "config": {
          "label": "Subscribe",
          "placeholder": "",
          "allowTargetBlank": false,
          "select": null
        }
      },
      "theme": {
        "type": "Select",
        "config": {
          "label": "Theme",
          "placeholder": "",
          "options": ["Red", "Pink", "Blue", "Green", "Yellow", "White"],
          "default_value": "Red"
        }
      },
      "available": {
        "type": "Boolean",
        "config": {
          "label": "Available",
          "default_value": false,
          "placeholder_true": "true",
          "placeholder_false": "false"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": { "choices": {} }
      }
    }
  }
}
